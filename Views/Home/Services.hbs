  <!-- Marketing messaging and featurettes
  ================================================== -->
  <!-- Wrap the rest of the page in another container to center all the content. -->

  <div class="container marketing">




    <!-- START THE FEATURETTES -->


    <div class="row featurette div-Hom-Info">
      <div class="col-md-7">
        <h2 class="featurette-heading">Contact J&J AutoCare Now
        </h2>
        <p class="lead">General Mechanics, No Appointment Necessary.
        <div>
          <p class="lead">
          <ul>
            {{#each Servicio as |Servicios|}}
              <li>{{Servicios.Descripcion}}  ${{Servicios.Precio}}</li>
            {{/each}}
            
          </ul>
          </p>
        </div>

        </p>

      </div>
      <div class="col-md-5">
        <img width="90%" height="90%" style="margin-top: 4%;" src="/Assets/img/ImageLocal.jpg" alt="">

      </div>
    </div>

  </div>




  <div class="container" id="#Form">
    <hr class="featurette-divider">
    <section class="form-section card-body">
      <div class="card-header card-title">
        <h2>Request Service</h2>

      </div>

      <section class="form-section-body">
        <section class="form-service">

          <form class="row g-4 form-service-child  " method="POST"action="/RequestService" >
            <div class="col-md-4">
              <label for="nombre" class="form-label text-terms">First name</label>
              <input type="text" class="form-control" name="nombre" id="nombre" placeholder="First Name" value="{{nombre}}"
                required>
              <div class="valid-feedback">
                {{Message}}
              </div>
            </div>
            <div class="col-md-4">
              <label for="apellido" class="form-label text-terms">Last name</label>
              <input type="text" class="form-control " name="apellido" id="apellido" placeholder="Last Name" value="{{apellido}}" required>
              <div class="valid-feedback">
               {{Message}}
              </div>
            </div>
            <div class="col-md-5">
              <label for="servicio" class="form-label text-terms">Service</label>
              <select class="form-select " id="servicio" name="servicio"
                aria-describedby="validationServer04Feedback"  required>
                <option selected disabled value="">Choose...</option>
                {{#each Servicio as |Servicios|}}
                <option value="">{{Servicios.Descripcion}}</option>
                {{/each}}
                <option>Personalized</option>
              </select>
              <div id="validationServer04Feedback" class="invalid-feedback">
                {{Message}}
              </div>
            </div>

            <div class="col-md-8">
              <label for="descripcion" class="form-label text-terms" >Description</label>
              <textarea type="text" class="form-control " id="descripcion" name="descripcion"
                aria-describedby="validationServer03Feedback" required placeholder="Description... " >{{descripcion}}</textarea>
              <div id="validationServer03Feedback" class="invalid-feedback">
               {{Message}}
              </div>
            </div>
            <div class="col-md-5">
              <label for="correo" class="form-label text-terms">Email</label>
              <input type="Email" class="form-control " id="correo" placeholder="Email" name="correo" required value="{{correo}}">
              <div class="valid-feedback">
                {{Message}}
              </div>
            </div>
            <div class="col-12">
              <div class="form-check">
                <input class="form-check-input " type="checkbox" value="" id="invalidCheck3"
                  aria-describedby="invalidCheck3Feedback" required>
                <label class="form-check-label text-terms" for="invalidCheck3">
                  Accept Terms and Conditions
                </label>
                <div id="invalidCheck3Feedback" class="invalid-feedback">
                  {{MessageT}}
                </div>
              </div>
            </div>
            <div class="col-md-8">
              <button class="btn btn-primary form-control" type="submit">Submit</button>
            </div>




          </form>
        </section>
        <section class="terms-service form-service-child">

          <div class="card-body">
            <div class="card-header card-title">
              <h2>Terms and conditions</h2>
            </div>
            <div class="text-terms">
              <p>Agree to terms and conditions
                You must agree before submitting.</p>
            </div>

          </div>


        </section>
      </section>


    </section>
  </div>



